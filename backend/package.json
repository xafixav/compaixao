{
  "name": "users",
  "version": "1.0.0",
  "description": "database backend",
  "main": "./build/server",
  "scripts": {
    "start": "node .",
    "test": "mocha -r ts-node/register ./src/tests/*$NAME*.{test,spec}.ts -t 10000 --exit",
    "test:coverage": "nyc npm run test",
    "test:coverage:json": "nyc --reporter=json-summary npm run test",
    "dev": "nodemon src/server.ts",
    "db:reset:start": "npm run db:reset && nodemon src/server.ts",
    "build": "npx tsc",
    "build-windows": "npx tsc && npm run db:reset:start",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "prestart": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xafixav/manage-sell-app.git"
  },
  "keywords": [
    "manage",
    "sell",
    "app"
  ],
  "author": "xafixav and OctavioLopes",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xafixav/manage-sell-app/issues"
  },
  "homepage": "https://github.com/xafixav/manage-sell-app#readme",
  "dependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "4.3.0",
    "@types/chai-http": "4.2.0",
    "@types/crypto-js": "^4.1.1",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/morgan": "^1.9.3",
    "@types/sequelize": "^4.28.14",
    "@types/sinon": "10.0.11",
    "axios": "^1.1.3",
    "bcryptjs": "^2.4.3",
    "chai": "4.3.6",
    "chai-http": "4.3.0",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "jest": "^29.2.2",
    "joi": "^17.7.0",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^9.2.2",
    "morgan": "^1.10.0",
    "mysql2": "2.3.3",
    "nodemon": "^2.0.20",
    "nyc": "15.1.0",
